{
    "name": "kerigard/laravel-mix-token",
    "description": "Tracking changes in files generated with Laravel Mix",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislav Sidelnikov",
            "email": "kerihoros@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.3",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Kerigard\\MixToken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerigard\\MixToken\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
